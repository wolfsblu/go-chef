{
	"name": "webapp",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"generate": "npx openapi-typescript ../api/openapi.yml -o ./src/lib/api/schema.d.ts",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
