		"@types/luxon": "^3.6.2",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.3.1",
		"flowbite-svelte-icons": "^2.1.0",
		"luxon": "^3.6.1",
		"openapi-fetch": "^0.14.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-dnd-action": "^0.9.61",
